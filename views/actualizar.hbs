{{>header}}
<p>Bienvenido a la pagina de inicio</p>
<form action="/actualizar" method="post" >
<div class="form-group"></
  <label for="formGroupExampleInput">Nombre</label>
  <input type="text" class="form-control" name="nombre" value="{{nombre}}" placeholder="Nombre de usuario" required>
<div class="row">
  <div class="col">
    <label for="formGroupExampleInput">Nota 1</label>
    <input type="text" class="form-control" name="matematicas" value="{{matematicas}}" placeholder="Nota 1" required>
  </div>
  <div class="col">
      <label for="formGroupExampleInput">Nota 2</label>
      <input type="text" class="form-control" name="ingles" value="{{ingles}}" placeholder="Nota 2" required>
  </div>
  <div class="col">
      <label for="formGroupExampleInput">Nota 3</label>
      <input type="text" class="form-control" name="programacion" value="{{programacion}}" placeholder="Nota 3" required>
</div>
</div>
<br>
<button class="btn btn-primary">Registrar</button>
</div>
</form>



{{>footer}}